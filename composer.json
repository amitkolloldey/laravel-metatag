{
    "name": "amitkolloldey/laravel-metatag",
    "description": "Dynamically add meta tags to Laravel application",
    "keywords": [
        "laravel meta",
        "eloquent meta",
        "html meta package",
        "laravel meta package",
        "laravel 7 meta",
        "html metatags"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Amit Kollol Dey",
            "email": "amitkolloldey@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/database": ">= 5.0|^6.0|^7.0",
        "illuminate/support": ">= 5.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "5.*|4.*",
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "AmitKD\\LaravelMetatag\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AmitKD\\LaravelMetatag\\Tests\\": "tests/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "AmitKD\\LaravelMetatag\\Providers\\MetaServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    }
}

